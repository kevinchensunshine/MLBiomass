/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var sv_gol = ee.ImageCollection("users/kevinlc3/denmark_wheat_smoothed");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
var visualization = {
  min: 0.0,
  max: 0.3,
  bands: ['smoothed_B4', 'smoothed_B3', 'smoothed_B2'],
};
var sz = sv_gol.size().getInfo()
var ls = sv_gol.toList(sz)

function addImage(image) { // display each image in collection
  var id = image.id
  var image = ee.Image(image.id)
  Map.addLayer(image)
}

sv_gol.evaluate(function(img) {  // use map on client-side
  img.features.map(addImage)
})

Map.centerObject(sv_gol)